              "paragraph_title": [
                {
                  "type": "heading2",
                  "text": "Worn Wear is Patagonia's program that keeps gear in action longer.",
                  "spans": []
                }
              ],
              "paragraph_text": [
                {
                  "type": "paragraph",
                  "text": "Why extend the life of gear? Because the best thing we can do for the planet is get more use out of stuff we already own, cutting down on consumption.",
                  "spans": []
                },
                {
                  "type": "paragraph",
                  "text": "Join us to repair, share and recycle your gear.",
                  "spans": []
                }
              ]
            }
